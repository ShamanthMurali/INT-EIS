        <mxCell id="RWugV7pzo6tbbd6pbP86-54" value="Pollution Damage and others" style="rounded=1;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="1800" y="640" width="120" height="40" as="geometry" />
        </mxCell>
        <mxCell id="RWugV7pzo6tbbd6pbP86-60" value="Controls" style="swimlane;startSize=40;" vertex="1" collapsed="1" parent="1">
          <mxGeometry x="1320" y="700" width="120" height="40" as="geometry">
