        <mxCell id="RWugV7pzo6tbbd6pbP86-69" value="Controls" style="swimlane;startSize=40;" vertex="1" collapsed="1" parent="1">
          <mxGeometry x="1800" y="700" width="120" height="40" as="geometry">
            <mxRectangle x="1920" y="700" width="400" height="320" as="alternateBounds" />
