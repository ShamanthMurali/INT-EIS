        <mxCell id="w9T0O8eBJ9PRjbyhEmXW-44" value="&lt;div&gt;Physical &lt;br&gt;&lt;/div&gt;&lt;div&gt;Security Controls&lt;/div&gt;" style="swimlane;startSize=40;fontFamily=Helvetica;fontSize=12;fontColor=#000000;align=center;strokeColor=#000000;fillColor=#ffffff;html=1;" parent="1" vertex="1" collapsed="1">
          <mxGeometry x="3780" y="395" width="120" height="40" as="geometry">
